[Daemon]
# Comma-separated list of plugins to load, no spaces
# Available plugins: L3Agent, DHCPAgent, Dump
plugins = L3Agent,DHCPAgent

rpchost = <%= @rabbit_ipaddress %>
pidfile = /var/run/rpcdaemon.pid
logfile = /var/log/rpcdaemon.log
loglevel = INFO

[L3Agent]
conffile = /etc/quantum/quantum.conf
loglevel = INFO

[DHCPAgent]
conffile = /etc/quantum/quantum.conf
loglevel = INFO

[Dump]
# Only writes to DEBUG
loglevel = DEBUG
